<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>TimeSea</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css"
    />
    <link rel="stylesheet" href="/style/userLogIn.css" />
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container">
        <a class="navbar-brand" href="/">TimeSea</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <div class="search-container me-2"></div>
        </div>
      </div>
    </nav>

    <div class="container login-container">
      <a class="login-title" href="/signUp">
        <h2>SignUp | <span>Login</span></h2>
      </a>
      <form action="/login" method="post">
        <% if (message) { %>
        <div class="alert alert-danger"><%= message %></div>
        <% } %>
        <div class="mb-3">
          <label for="email" class="form-label">Email:</label>
          <input
            type="email"
            class="form-control"
            name="email"
            id="email"
            placeholder="Type here"
            required
          />
        </div>
        <div class="mb-3">
          <label for="password" class="form-label">Password:</label>
          <div class="input-group">
            <input
              type="password"
              class="form-control"
              name="password"
              id="password"
              placeholder="Type here"
              required
            />
            <button
              class="btn btn-outline-secondary"
              type="button"
              id="togglePassword"
            >
              <i class="bi bi-eye" id="togglePasswordIcon"></i>
            </button>
          </div>
        </div>
        <button type="submit" class="btn btn-dark w-100">Login</button>
        <div class="text-center mt-2">
          <a href="/forgotPassword" class="text-decoration-none"
            >Forgot password?</a
          >
        </div>
        <div class="text-center mt-3">
          Sign in with
          <p>or continue with Google</p>
          <a href="/auth/google" class="btn btn-outline-dark">
            <img
              alt="Google"
              src="data:image/png;base64,"
              width="20"
            />
            Google
          </a>
        </div>
        <div class="text-center mt-3">
          <small
            >By clicking continue, you agree to our
            <a href="#">Terms of Service</a> and
            <a href="#">Privacy Policy</a></small
          >
        </div>
      </form>
    </div>

    <script>
      document
        .getElementById("togglePassword")
        .addEventListener("click", function (e) {
          const passwordField = document.getElementById("password");
          const toggleIcon = document.getElementById("togglePasswordIcon");

          // Toggle the type attribute
          const type =
            passwordField.getAttribute("type") === "password"
              ? "text"
              : "password";
          passwordField.setAttribute("type", type);

          // Toggle the icon
          toggleIcon.classList.toggle("bi-eye");
          toggleIcon.classList.toggle("bi-eye-slash");
        });
    </script>

    <footer class="footer">
      <div class="container">
        <div class="row">
          <div class="col-md-3">
            <h5>TimeSea</h5>
            <div class="mt-3">
              <a href="#" class="text-dark me-2"
                ><i class="bi bi-facebook"></i
              ></a>
              <a href="#" class="text-dark me-2"
                ><i class="bi bi-linkedin"></i
              ></a>
              <a href="#" class="text-dark me-2"
                ><i class="bi bi-youtube"></i
              ></a>
              <a href="#" class="text-dark"><i class="bi bi-instagram"></i></a>
            </div>
            <p class="mt-2"><small>Copyright 2024 Â© Time Sea</small></p>
          </div>
          <div class="col-md-3">
            <h5>About</h5>
            <ul class="list-unstyled">
              <li>
                <a href="#" class="text-dark text-decoration-none">About Us</a>
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none">Category</a>
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none">Blog</a>
              </li>
            </ul>
          </div>
          <div class="col-md-3">
            <h5>Information</h5>
            <ul class="list-unstyled">
              <li>
                <a href="#" class="text-dark text-decoration-none"
                  >Help Center</a
                >
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none">Refund</a>
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none"
                  >Contact Us</a
                >
              </li>
            </ul>
          </div>
          <div class="col-md-3">
            <h5>Users</h5>
            <ul class="list-unstyled">
              <li>
                <a href="#" class="text-dark text-decoration-none">Login</a>
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none">Register</a>
              </li>
              <li>
                <a href="#" class="text-dark text-decoration-none">My Orders</a>
              </li>
            </ul>
          </div>
        </div>
      </div>

      <%-include("footerInclude")%>
    </footer>
  </body>
</html>
